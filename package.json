{
  "name": "nodejs-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run prod",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "server:prod": "pm2-runtime -i 0 .dist/index.js",
    "dev": "NODE_ENV=development nodemon --exec npm-run-all server:dev",
    "server:dev": "babel-node ./src/index.js",
    "clean": "rimraf .dist",
    "build": "babel ./src --out-dir .dist"
  },
  "author": "Lucas Pan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "helmet": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "pino": "^6.13.0",
    "pino-pretty": "^5.1.2",
    "pm2": "^5.1.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "babel-eslint": "^10.1.0",
    "nodemon": "^2.0.12"
  }
}
